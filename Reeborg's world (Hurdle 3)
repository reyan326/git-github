def turn_right():
    turn_left()
    turn_left()
    turn_left()

def one_round():
    turn_left()
    move()
    turn_right()
    move()
    turn_right()
    move()
    turn_left()
   # turn_left()
       
    
while not at_goal():
    if wall_in_front():
        one_round()
    elif front_is_clear():
        move()
